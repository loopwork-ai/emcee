{
	"mcpServers": {
		"weather": {
			"command": "emcee",
			"args": ["https://api.weather.gov/openapi.json"]
		},
		"x (formerly twitter)": {
			"command": "emcee",
			"args": ["https://api.x.com/2/openapi.json", "--bearer-auth=$X_API_KEY"]
		},
		"openai": {
			"command": "emcee",
			"args": [
				"https://raw.githubusercontent.com/openai/openai-openapi/refs/heads/master/openapi.yaml",
				"--bearer-auth=$OPEN_AI_API_KEY"
			]
		}
	}
}
